<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pro.financial.management.dao.RevenueTypeDao">
  <resultMap id="BaseResultMap" type="com.pro.financial.management.dao.entity.RevenueTypeEntity">
    <id column="revenue_type_id" jdbcType="INTEGER" property="revenueTypeId" />
    <result column="revenue_type_name" jdbcType="VARCHAR" property="revenueTypeName" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>
  <sql id="Base_Column_List">
    revenue_type_id, revenue_type_name, remark
  </sql>
  <select id="getType" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"/>
    from revenue_type
    <if test="keyWords != null and keyWords !=''">
      where revenue_type_name like "%${keyWords}%"
    </if>
  </select>
</mapper>